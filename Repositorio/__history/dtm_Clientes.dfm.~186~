  object dst_RelatorioCliente: TSQLDataSet
    CommandText = 
      'select  '#13#10'   cliente.cli_id_pessoa,'#13#10'   pessoa.pes_id_interno,'#13#10 +
      '   pessoa.pes_tp_cadastro,'#13#10'   pessoa.pes_tp_pessoa,'#13#10'   pessoa.' +
      'pes_situacao,'#13#10'   pessoa.pes_rsocial,'#13#10'   pessoa.pes_fantasia,'#13#10 +
      '   pessoa.pes_cnpj_cpf,'#13#10'   pessoa.pes_insc_rg,'#13#10'   pessoa_ender' +
      'eco.pe_tipo,'#13#10'   pessoa_endereco.pe_endereco,'#13#10'   pessoa_enderec' +
      'o.pe_numero,'#13#10'   pessoa_endereco.pe_complemento,'#13#10'   pessoa_ende' +
      'reco.pe_bairro,'#13#10'   pessoa_endereco.pe_id_cidade,'#13#10'   pessoa_end' +
      'ereco.pe_cep,'#13#10'   pessoa_telefone.pt_numero,'#13#10'   pessoa_email.pe' +
      'm_email,'#13#10'   pessoa.pes_insert_date,'#13#10'   pessoa.pes_update_date'#13 +
      #10'from cliente'#13#10'   left outer join pessoa on (cliente.cli_id_pess' +
      'oa =  pessoa.pes_id)'#13#10'   left outer join pessoa_endereco on (cli' +
      'ente.cli_id_pessoa = pessoa_endereco.pe_id_pessoa and pessoa_end' +
      'ereco.pe_tipo = 1)'#13#10'   left outer join pessoa_telefone on (clien' +
      'te.cli_id_pessoa = pessoa_telefone.pt_id_pessoa and pessoa_telef' +
      'one.pt_tipo = 1)'#13#10'   left outer join pessoa_email on (cliente.cl' +
      'i_id_pessoa = pessoa_email.pem_id_pessoa and pessoa_email.pem_se' +
      'q = 1)'#13#10'where'#13#10'   cliente.cli_id_pessoa = :Id'
    DbxCommandType = 'Dbx.SQL'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'Id'
        ParamType = ptInput
      end>
    SQLConnection = dm_Conexao.Conn
    Left = 496
    Top = 408
